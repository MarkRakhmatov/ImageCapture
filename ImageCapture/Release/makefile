-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: FirstProject

# Tool invocations
FirstProject: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: Cross G++ Linker'
	arm-linux-gnueabihf-g++ -L/usr/arm-linux-gnueabihf/lib -o "FirstProject" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(CC_DEPS)$(C++_DEPS)$(EXECUTABLES)$(OBJS)$(C_UPPER_DEPS)$(CXX_DEPS)$(C_DEPS)$(CPP_DEPS) FirstProject
	-@echo ' '

post-build:
	-@echo 'Copy file to beaglebone'
	-bash -c "scp /home/mark/workspace/FirstProject/Release/FirstProject debian@192.168.7.2:FirstProject"
	-@echo ' '

.PHONY: all clean dependents post-build

-include ../makefile.targets
